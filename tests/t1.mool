class Test() {
	method int pow(n: int, m: int) { 
    var result: int = 0;
    if n != 0 then{
			result = m * this.pow(n-1, m);
		}else{
			result = 1;
		}
		return result;
  }

  method int m(x: int) {
		var r: int = this.pow(3, x);
		return r;
  }
	
	static void main() {
		var t: Test = new Test();
		print "" + t.m(3);
	}
}

